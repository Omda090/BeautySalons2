
@{
    ViewData["Title"] = "IndexVideo";
}

<h2>فيديوهات وصور تعريفية</h2>


<div id="carouselExampleIndicators" class="carousel slide w-75" style="box-shadow:0px 0px 5px #666;flex; margin-left:auto; margin-right:auto;" data-interval="2000" data-ride="carousel">
    <ol class="carousel-indicators">
        <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
        <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
        <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
    </ol>
    <div class="carousel-inner">
        <div class="carousel-item active">
            <img class="d-block w-100 img-thumbnail" src="/ExtraImages/+2.jpg" alt="First slide">
        </div>
        <div class="carousel-item">
            <img class="d-block w-100 img-thumbnail" src="/ExtraImages/+20.jpeg" alt="Second slide">
        </div>
        <div class="carousel-item">
            <img class="d-block w-100 img-thumbnail" src="/ExtraImages/+22.jpg" alt="Third slide">
        </div>
        <div class="carousel-item">
            <img class="d-block w-100 img-thumbnail" src="/ExtraImages/+25.jpeg" alt="Third slide">
        </div>
        <div class="carousel-item">
            <img class="d-block w-100 img-thumbnail" src="/Mix/1.jpg" alt="Third slide">
        </div>
        <div class="carousel-item">
            <img class="d-block w-100 img-thumbnail" src="/ExtraImages/+5.jpg" alt="Third slide">
        </div>
        <div class="carousel-item">
            <img class="d-block w-100 img-thumbnail" src="/Mix/3.jpg" alt="Third slide">
        </div>
        <div class="carousel-item">
            <img class="d-block w-100 img-thumbnail" src="/ExtraImages/+8.jpg" alt="Third slide">
        </div>
        <div class="carousel-item">
            <img class="d-block w-100 img-thumbnail" src="/ExtraImages/+22 (1).jpg" alt="Third slide">
        </div>
    </div>
    <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
</div>

<br>

